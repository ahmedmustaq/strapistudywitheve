{
  "kind": "collectionType",
  "collectionName": "ai_training_sessions",
  "info": {
    "singularName": "ai-training-session",
    "pluralName": "ai-training-sessions",
    "displayName": "AITrainingSession",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "topic": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::topic.topic"
    },
    "resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource"
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        "Easy",
        "Medium",
        "Hard"
      ]
    },
    "questions": {
      "type": "json"
    },
    "marking": {
      "type": "json"
    },
    "finalscore": {
      "type": "integer"
    },
    "feedback": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "InProgress",
        "Completed",
        "Abandoned",
        "MarkingInProgress",
        "MarkingCompleted"
      ]
    },
    "student": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::student.student"
    },
    "duration": {
      "type": "integer"
    },
    "noofquestions": {
      "type": "integer"
    },
    "questiontypes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::questiontype.questiontype"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "online",
        "paper"
      ]
    },
    "submission": {
      "type": "json"
    },
    "submission_file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "studyproject": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::studyproject.studyproject"
    },
    "assessment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::assessment.assessment"
    },
    "sessionduration": {
      "type": "integer"
    },
    "generationtype": {
      "type": "enumeration",
      "enum": [
        "AI",
        "Revision"
      ],
      "default": "AI"
    }
  }
}
